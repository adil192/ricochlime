{
    "dart.analysisExcludedFolders": [
        "submodules"
    ],
    "files.exclude": {
        "android/.gradle/": true,
        "android/.kotlin/": true,
        "linux/flutter/ephemeral/": true,
        "ricochlime.iml": true,
        "submodules/*/*": true,
        "**/.dart_tool/": true,
        "**/.idea/": true,
        "**/.flutter-plugins-dependencies": true,
    },
    "workbench.colorCustomizations": {
        "[Gnome Dark (GitHub)]": {
            "titleBar.activeBackground": "#192e1a",
            "activityBar.background": "#192e1a"
        },
        "[Gnome Light (GitHub)]": {
            "titleBar.activeBackground": "#cbe6cd",
            "activityBar.background": "#cbe6cd"
        }
    }
}
